<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>IPChecker</RootNamespace>
    <AssemblyName>Alex75.IPChecker</AssemblyName>
    <AssemblyVersion>#{package version}#-alpha</AssemblyVersion>
    <Version>0.1.1-alpha</Version>
    <PackageId>Alex75.IPChecker</PackageId>
    <Company>DreamSoft</Company>
    <Product>IP Checker</Product>
    <RepositoryType>GIT</RepositoryType>
    <Authors>Alessandro Piccione</Authors>
    <RepositoryUrl>https://github.com/alex75it/IP-Checker</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Objects.fs" />
    <Compile Include="Library.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Flurl.Http" Version="2.4.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="4.5.4" />
  </ItemGroup>

</Project>
